 "css": ".print-format{\n     line-height:1.5  !important;\n     /*margin-top:15px;*/\n     /*margin-bottom:55px !important;*/\n}\n.print-format td, .print-format th {\n\tpadding: 4px !important;\n\tfont-size:12px;\n}\n @page {\n      margin: 1.5cm;\n    }\n.item{\n    border:1px solid black;\n}",
